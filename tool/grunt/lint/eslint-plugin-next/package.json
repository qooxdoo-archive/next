{
  "name": "eslint-plugin-next",
  "version": "0.1.1",
  "homepage": "http://qooxdoo.org/",
  "description": "Eslint rules for Backbone.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run unit-test --coverage && npm run check-coverage",
    "lint": "eslint ./",
    "unit-test": "istanbul test --dir build/coverage node_modules/mocha/bin/_mocha tests/**/*.js -- --recursive --reporter dot",
    "check-coverage": "istanbul check-coverage --statement 100 --branch 100 --function 100 --lines 100",
    "report-coverage-html": "istanbul report --dir  build/coverage html"
  },
  "author": {
    "name": "Tobias Oberrauch",
    "email": "tobias.oberrauch@1und1.de"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qooxdoo/qooxdoo.git"
  },
  "bugs": {
    "url": "http://bugzilla.qooxdoo.org/"
  },
  "devDependencies": {
    "istanbul": "0.3.0",
    "mocha": "1.21.4",
    "eslint-tester": "^0.3.0",
    "eslint": ">=0.8.1"
  },
  "peerDependencies": {
    "eslint": ">=0.8.1"
  },
  "licenses": [
    {
      "type": "LGPL",
      "url": "http://www.gnu.org/licenses/lgpl.html"
    },
    {
      "type": "EPL",
      "url": "http://www.eclipse.org/org/documents/epl-v10.php"
    }
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "eslint-plugin",
    "eslintplugin",
    "next",
    "eslint"
  ],
  "//": "before publishing create README.md then this can be removed",
  "private": true
}
